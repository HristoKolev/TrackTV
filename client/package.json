{
  "name": "angular-webpack2-starter",
  "version": "3.0.0",
  "description": "Angular and Webpack 2 Starter",
  "author": "Qdouble <qdouble@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qdouble/angular-webpack2-starter.git"
  },
  "scripts": {
    "postinstall": "npm run build:dll",
    "rimraf": "rimraf",
    "webdev": "webpack-dev-server",
    "webpack": "webpack",
    "pretest": "npm run lint",
    "pretest:once": "npm run lint",
    "tslint": "tslint --type-check --project tsconfig.json",
    "lint": "npm run tslint \"src/app/**/*.ts\" ",
    "clean": "npm cache clean && npm run clean:compile && npm run rimraf -- node_modules doc typings coverage dist .awcache dll",
    "clean:dist": "npm run rimraf -- dist .awcache",
    "clean:dll": "npm run rimraf -- dll",
    "clean:compile": "npm run rimraf -- \"compiled\" ",
    "start": "npm run server:dev",
    "server:dev": "npm run webdev",
    "debug:build": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js",
    "build": "npm run build:prod",
    "build:aot": "npm run build:aot:prod",
    "build:aot:prod": "npm run clean:dist && webpack",
    "build:aot:dev": "npm run clean:dist && webpack",
    "build:dev": "npm run clean:dist && webpack",
    "build:dll": "npm run clean:dll && webpack",
    "build:prod": "npm run clean:dist && webpack",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand"
  },
  "dependencies": {
    "@angular-redux/store": "^6.5.4",
    "@angular/common": "^4.1.0",
    "@angular/compiler": "^4.1.0",
    "@angular/compiler-cli": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@angular/forms": "^4.1.0",
    "@angular/http": "^4.1.0",
    "@angular/platform-browser": "^4.1.0",
    "@angular/platform-browser-dynamic": "^4.1.0",
    "@angular/router": "^4.1.0",
    "@angular/tsc-wrapped": "^4.1.0",
    "@angularclass/idle-preload": "^1.0.4",
    "core-js": "2.4.1",
    "hammerjs": "~2.0.8",
    "redux": "^3.7.1",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.4.0",
    "zone.js": "^0.8.9"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.3.1",
    "@types/express": "~4.0.35",
    "@types/ip": "~0.0.29",
    "@types/jest": "^20.0.6",
    "@types/node": "~7.0.12",
    "angular-router-loader": "~0.6.0",
    "angular2-template-loader": "~0.6.2",
    "awesome-typescript-loader": "~3.1.2",
    "codelyzer": "~3.0.1",
    "compression": "1.6.2",
    "copy-webpack-plugin": "~4.0.1",
    "css-loader": "~0.28.0",
    "es6-promise": "~4.1.0",
    "es6-promise-loader": "~1.0.2",
    "html-webpack-plugin": "~2.28.0",
    "imports-loader": "~0.7.1",
    "ip": "~1.1.5",
    "istanbul-instrumenter-loader": "~0.2.0",
    "jest": "^20.0.4",
    "jest-preset-angular": "^2.0.5",
    "json-loader": "~0.5.4",
    "node-sass": "~4.5.2",
    "npm-run-all": "~4.0.2",
    "raw-loader": "~0.5.1",
    "rimraf": "~2.6.1",
    "sass-loader": "^6.0.5",
    "source-map-loader": "~0.2.1",
    "string-replace-loader": "~1.2.0",
    "to-string-loader": "~1.1.5",
    "ts-helpers": "~1.1.2",
    "ts-node": "~3.0.2",
    "tslint": "~5.3.2",
    "tslint-loader": "~3.5.2",
    "typescript": "~2.3.2",
    "webpack": "~2.6.0",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-middleware": "~1.10.1",
    "webpack-dev-server": "~2.4.2",
    "webpack-md5-hash": "^0.0.5"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/qdouble/angular-webpack2-starter/issues"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/test/setupJest.ts",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./src/tsconfig.spec.json"
      }
    }
  }
}
